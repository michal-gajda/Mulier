root = true

[*]
end_of_line = crlf
insert_final_newline = true

[*.cs]
charset = utf-8
indent_style = space
indent_size = 4

[*.{csproj,props}]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = false

[nuget.config]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = false

[*.sln]
charset = utf-8
indent_style = tab
indent_size = 2
